{
    "name": "vscode-files-explorer",
    "displayName": "Files Explorer",
    "description": "Supercharge for Files Explorer view",
    "version": "0.0.1",
    "publisher": "HenryTran",
    "author": {
        "name": "Henry Tráº§n",
        "email": "tran.trong@outlook.com",
        "url": "https://kingdark.org/"
    },
    "engines": {
        "vscode": "^1.75.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./scripts/extension.js",
    "contributes": {
        "commands": [{
            "command": "vscode-files-explorer.commands.expand-all-folder",
            "title": "Expand all folder",
            "icon":{
                "light": "./media/light/expand-all.svg",
                "dark": "./media/dark/expand-all.svg"
            }
        }],
        "menus": {
            "view/title": [
                {
                    "command": "vscode-files-explorer.commands.expand-all-folder",
                    "when": "view == 'workbench.explorer.fileView'",
                    "group": "navigation"
                }
            ]
        },
        "views": {
            "explorer": [
                {
                    "id": "vscode-files-explorer.views.files-explorer-list",
                    "name": "Files Explorer (Listview)"
                }
            ]
        }
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.75.0",
        "@types/glob": "^8.0.1",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "eslint": "^8.33.0",
        "glob": "^8.1.0",
        "mocha": "^10.1.0",
        "typescript": "^4.9.4",
        "@vscode/test-electron": "^2.2.2"
    }
}